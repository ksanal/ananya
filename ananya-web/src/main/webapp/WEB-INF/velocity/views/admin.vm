<html>
<head>
    <title>Ananya Test</title>
    <script type="text/javascript" src="${bundle.web.url}js/admin/jquery-1.6.2.min.js"></script>
    <script type="text/javascript" src="${bundle.web.url}js/admin/jquery-ui-1.8.16.custom.min.js"></script>
    <script type="text/javascript" src="${bundle.web.url}js/admin/controller.js"></script>
    <link rel="stylesheet" type="text/css" href="${bundle.web.url}css/admin.css" media="screen, projection, print"/>
</head>
<body>
<div id="workerPresenter_form">
    <h3>Registered FLWs</h3>
    <table>
        <thead>
        <tr>
            <th>id</th>
            <th>msisdn</th>
            <th>status</th>
            <th>district</th>
            <th>block</th>
            <th>panchayat</th>
        </tr>
        </thead>
        #foreach($workerPresenter in $workerPresenters)
            <tr>
                <td>$workerPresenter.getId()</td>
                <td>$workerPresenter.getMsisdn()</td>
                <td>$workerPresenter.getStatus()</td>
                <td>$workerPresenter.getDistrict()</td>
                <td>$workerPresenter.getBlock()</td>
                <td>$workerPresenter.getPanchayat()</td>
            </tr>
        #end
    </table>
</div>
</body>

</html>
#vxmlHeader()
<script srcexpr="$callerData"/>
<form id="landing_page">
    <block>
        <if cond="callerData.isRegistered == 'true'">
            <goto next="$nextFlow"/>
            <else/>
            #play("${bundle.audioFileBase}${bundle.jobaid.welcome}")
            #play("${bundle.audioFileBase}${bundle.jobaid.detail}")
            #play("${bundle.audioFileBase}${bundle.jobaid.need.to.register}")
            <goto next="$registerFlow"/>
        </if>
    </block>
</form>
#vxmlFooter()
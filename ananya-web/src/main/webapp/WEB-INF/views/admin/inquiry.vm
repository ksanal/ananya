<link href="css/prettify.css" type="text/css" rel="stylesheet"/>

<div class="row-fluid">
    <div class="span12">
        <div class="page-header">
            <h2>Caller Inquiry</h2>
        </div>
    </div>
</div>

<div class="row-fluid">
    <div class="span6">
        <form class="form-search" id="msisdn_form">
            <input type="text" class="input-medium search-query" name="msisdn" id="msisdn" placeholder="Search">
            <label class="control-label" for="msisdn"><span id="msisdn_info" class="label">Enter MSISDN</span></label>
        </form>
    </div>
</div>

<div class="row-fluid">
    <div class="span4">
        <h3>Academy Calls:</h3>
    </div>
</div>
<div class="row-fluid">
    <div class="span12" id='academy_calls_table_error'></div>
</div>
<div class="row-fluid">
    <div class="span12" style="overflow: auto;">
        <table class='table table-striped table-bordered' id='academy_calls_table' style="display: none">
            <thead></thead>
            <tbody></tbody>
        </table>
    </div>
</div>
<div class="row-fluid">
    <div class="span9" id='academy_calls_table_pagination'></div>
    <div class="span3" id='academy_calls_table_go_to_page'></div>
</div>

<div class="row-fluid">
    <div class="span4">
        <h3>Kunji Calls:</h3>
    </div>
</div>
<div class="row-fluid">
    <div class="span12" id='kunji_calls_table_error'></div>
</div>
<div class="row-fluid">
    <div class="span12" style="overflow: auto;">
        <table class='table table-striped table-bordered' id='kunji_calls_table' style="display: none">
            <thead></thead>
            <tbody></tbody>
        </table>
    </div>
</div>
<div class="row-fluid">
    <div class="span9" id='kunji_calls_table_pagination'></div>
    <div class="span3" id='kunji_calls_table_go_to_page'></div>
</div>

<div class="row-fluid">
    <div class="span4">
        <h3>Call Details:</h3>
    </div>
</div>
<div class="row-fluid">
    <div class="span12" id='call_details_table_error'></div>
</div>
<div class="row-fluid">
    <div class="span12" style="overflow: auto;">
        <table class='table table-striped table-bordered' id='call_details_table' style="display: none">
            <thead></thead>
            <tbody></tbody>
        </table>
    </div>
</div>
<div class="row-fluid">
    <div class="span9" id='call_details_table_pagination'></div>
    <div class="span3" id='call_details_table_go_to_page'></div>
</div>

<div class="row-fluid">
    <div class="span4">
        <h3>Caller Data to IVR:</h3>
    </div>
</div>
<div class="row-fluid">
    <div class="span12">
        <pre id="caller_data_to_ivr" class="prettyprint linenums"></pre>
    </div>
</div>

<script type="text/javascript" src="js/prettify.js"></script>
<script type="text/javascript" src="js/data-grid.js"></script>
<script type="text/javascript" src="js/inquiry.js"></script>
